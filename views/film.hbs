 <div class= "container">
   <div class="poster">
     <img src="{{oneFilm.image}}" alt="" />
   </div>
   <div class="textContainer">
   <div>
     <h2>{{oneFilm.title}}</h2>
   </div>
   <div>
     <h3>Director: {{oneFilm.director}}</h3>
   </div>
   <div>
     <h4>Starring:</h4>
     <ul>
       <li>{{oneFilm.stars.[0]}}</li>
       <li>{{oneFilm.stars.[1]}}</li>
       <li>{{oneFilm.stars.[2]}}</li>
     </ul>
   </div>
   <div>
     <p>{{oneFilm.description}}</p>
   </div>
   <div>
     <p>{{oneFilm.showtimes.[0]}} | {{oneFilm.showtimes.[1]}} | 
         {{oneFilm.showtimes.[2]}} | {{oneFilm.showtimes.[3]}} | 
         {{oneFilm.showtimes.[4]}}</p>
   </div>
   </div>
   <br /><br /><br />

 </div> 